    "dock": "left"
  },
  "outline_panel": {
    "dock": "right"
  },
  "project_panel": {
    "entry_spacing": "comfortable",
    "dock": "right"
  },
  "tabs": {
    "close_position": "right",
    "file_icons": true,
    "git_status": true,
    "show_close_button": "hover",
